# meh - alt + shift + ctrl
# hyper - alt + shift + ctrl + cmd

meh - r : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"
meh - f : yabai -m window --toggle zoom-fullscreen
meh - p : yabai -m window --toggle zoom-parent

meh - h : yabai -m window --warp west
meh - j : yabai -m window --warp south
meh - k : yabai -m window --warp north
meh - l : yabai -m window --warp east

hyper - h : yabai -m window --focus west || yabai -m display --focus west
hyper - j : yabai -m window --focus south || yabai -m display --focus south
hyper - k : yabai -m window --focus north || yabai -m display --focus north
hyper - l : yabai -m window --focus east || yabai -m display --focus east

meh - left : yabai -m window --space prev; skhd --key "ctrl - left"
meh - right : yabai -m window --space next; skhd --key "ctrl - right"
hyper - left : yabai -m window --display prev; yabai -m display --focus prev
hyper - right : yabai -m window --display next; yabai -m display --focus next

meh - 1 : yabai -m window --space 1; skhd --key "ctrl - 1"
meh - 2 : yabai -m window --space 2; skhd --key "ctrl - 2"
meh - 3 : yabai -m window --space 3; skhd --key "ctrl - 3"
meh - 4 : yabai -m window --space 4; skhd --key "ctrl - 4"
meh - 5 : yabai -m window --space 5; skhd --key "ctrl - 5"
meh - 6 : yabai -m window --space 6; skhd --key "ctrl - 6"
meh - 7 : yabai -m window --space 7; skhd --key "ctrl - 7"
meh - 8 : yabai -m window --space 8; skhd --key "ctrl - 8"
meh - 9 : yabai -m window --space 9; skhd --key "ctrl - 9"
hyper - 1 : yabai -m window --display 1; yabai -m display --focus 1
hyper - 2 : yabai -m window --display 2; yabai -m display --focus 2
hyper - 3 : yabai -m window --display 3; yabai -m display --focus 3
hyper - 4 : yabai -m window --display 4; yabai -m display --focus 4
hyper - 5 : yabai -m window --display 5; yabai -m display --focus 5
hyper - 6 : yabai -m window --display 6; yabai -m display --focus 6
hyper - 7 : yabai -m window --display 7; yabai -m display --focus 7
hyper - 8 : yabai -m window --display 8; yabai -m display --focus 8
hyper - 9 : yabai -m window --display 9; yabai -m display --focus 9
